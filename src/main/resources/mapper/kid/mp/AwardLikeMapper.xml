<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.etammag.dreamlighter.mapper.kid.mp.AwardLikeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.etammag.dreamlighter.entity.kid.db.AwardLike">
        <result column="award_id" property="awardId" />
        <result column="kid_id" property="kidId" />
    </resultMap>
    <select id="cat" resultType="java.lang.Integer">
        select 1 from kid_award_like where award_id = #{awardId} and kid_id = #{kidId} limit 1;
    </select>

</mapper>
